#include<iostream>
#include<conio.h>
using namespace std;
int main()
{
	int a, b, c, r;
	cout << "Enter number of rows" << endl;
	cin >> r;
	cout << "Enter number of coloumns" << endl;
	cin >> c;

	if (r == 0)
		cout << "Invalid";
	else if (c == 0)
		cout << "Invalid";
	else
	{
		for (a = 0; a < r; a++)
		{
			for (b = 0; b < c; b++)
			{
				cout << "*";
			}
			cout << endl;
		}
	}
	_getch();
	return 0;
}
