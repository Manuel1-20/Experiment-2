#include<iostream>
#include<conio.h>
using namespace std;
int main()
{
	int a, b, c;

	cout << "Enter the first number" << endl;
	cin >> a;
	cout << "Enter the second number" << endl;
	cin >> b;
	cout << "Enter the third number" << endl;
	cin >> c;

	if (a >= b && a >= c)
		cout << "the greatest number is " << a;
	else if (b >= a && b >= c)
		cout << "the greatest number is " << b;
	else
		cout << "the greatest number is " << c;
	
	_getch();
	return 0;
}
